	@LEVMBRS("Markets",0),@LEVMBRS("Product",0),@DESCENDANTS(Combos), @DESCENDANTS("Manual - Audit IDs"), Actual';

create or replace filter RFEM.Main.ELIMBF
	read on 'Actual,Budget',
	write on '@LEVMBRS(Accounts,0), @LEVMBRS("Product Type",0),
	@LEVMBRS("Markets",0),@LEVMBRS("Product",0),@DESCENDANTS(Combos), @DESCENDANTS("Manual - Audit IDs"), Budget';

